#
#  For working with the dotfiles repository
#
if [ -d ~/.dotfiles ]; then
   alias DOT='git --work-tree=$HOME --git-dir=$HOME/.dotfiles/'
fi


#
# Move up
#
alias ..='cd ..'

alias yay='https_proxy=socks5://127.0.0.1:20170 http_proxy=socks5://127.0.0.1:20170 yay'
alias proxy='https_proxy=socks5://127.0.0.1:20170 http_proxy=socks5://127.0.0.1:20170'
alias ejectusb='udisksctl unmount -b /dev/sda1; udisksctl power-off -b /dev/sda'
